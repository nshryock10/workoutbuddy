{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@src/*": ["src/*"],
        "@assets/*": ["assets/*"],
        "@assets/constants/*": ["assets/constants/*"]
      },
      "allowJs": true
    },
    "include": [
      "src/**/*",
      "assets/**/*",
      "App.tsx"
, "src", "src"    ]
  }